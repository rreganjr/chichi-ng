{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"cli": {
		"analytics": false
	},
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"chichi-ng": {
			"projectType": "library",
			"root": "projects/chichi-ng",
			"sourceRoot": "projects/chichi-ng/src",
			"prefix": "cc",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/chichi-ng/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/chichi-ng/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/chichi-ng/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/chichi-ng/src/test.ts",
						"tsConfig": "projects/chichi-ng/tsconfig.spec.json",
						"karmaConfig": "projects/chichi-ng/karma.conf.js"
					}
				}
			},
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			}
		}
	},
	"defaultProject": "chichi-ng"
}